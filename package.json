{
  "name": "appsintomas",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "expo": "~48.0.9",
    "expo-font": "~11.1.1",
    "expo-linear-gradient": "^12.1.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-google-fonts": "^0.0.0",
    "tailwind-react-native-classnames": "^1.5.1",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true,
  "expo": {
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "web": {
      "build": {
        "postPublish": [
          {
            "config": "tailwind.config.js",
            "command": "npx tailwindcss-cli@latest build src/tailwind.css -o public/index.css"
          }
        ]
      }
    }
  }
}
